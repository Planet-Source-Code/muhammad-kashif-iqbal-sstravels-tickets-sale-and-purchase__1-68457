Private Sub Toolbar1_ButtonClick(ByVal Button As MSComctlLib.Button)
Select Case Button.Key
            Case "airline"
                frmAirLine.Show
                frmAirLine.ZOrder
                
            Case "purchase"
                frmPurchase.Show
                frmPurchase.ZOrder
                
            Case "stock"
                frmStock.Show
                frmStock.ZOrder
                
            Case "sales"
                frmSales.Show
                frmSales.ZOrder
                
            Case "exit"
                End
               
            Case "log"
                ans = MsgBox("Are you sure do you want Log Off?", vbQuestion + vbYesNo, "Log Off")
                If ans = vbYes Then
                    frmLogin.Show
                    Me.Hide
                Else
                    Exit Sub
                End If
            
            Case "customers"
                frmCustomer.Show
                
           End Select
 End Sub
Private Sub Toolbar1_ButtonMenuClick(ByVal ButtonMenu As MSComctlLib.ButtonMenu)
Dim rsSecurity As New ADODB.Recordset
Dim rsLocation As New ADODB.Recordset
Dim level As String
Dim location As String
' ----- Get User Level
    If rsSecurity.State = 1 Then Set rsSecurity = Nothing
    
        rsSecurity.Open "SELECT * from [Users] where [UserName] ='" & sb1.Panels(5).Text & "'", Cn, adOpenDynamic, adLockPessimistic
    
            level = rsSecurity.Fields("UserLevel")

Select Case ButtonMenu.Key
    
    Case "purchaserep"
        
        If level = "Salesman" Then
            MsgBox "You are restricted in this module!", vbCritical, "Restricted"
                    Exit Sub
                    
        Else
            frmPurchaseReports.Show
        End If
        
    Case "salerep"
        
        If level = "Salesman" Then
            MsgBox "You are restricted in this module!", vbCritical, "Restricted"
                    Exit Sub
                    
        Else
            frmSalesReport.Show
        End If
        
          
End Select
End Sub
